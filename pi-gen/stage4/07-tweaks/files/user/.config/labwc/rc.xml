<?xml version="1.0"?>
<openbox_config xmlns="http://openbox.org/3.4/rc">
  <keyboard>
    <keybind key="XF86_MonBrightnessUp">
      <action name="Execute" command="rpi-backlight up"/>
    </keybind>
    <keybind key="XF86_MonBrightnessDown">
      <action name="Execute" command="rpi-backlight down"/>
    </keybind>
  </keyboard>
<theme><font place="ActiveWindow"><name>PibotoLt</name><size>16</size><weight>Normal</weight><slant>Normal</slant></font><font place="InactiveWindow"><name>PibotoLt</name><size>16</size><weight>Normal</weight><slant>Normal</slant></font><name>PiXnoir_l</name></theme></openbox_config>
